{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"title": "VirtualBox manifest",
	"type": "object",
	"properties": {
		"volume": {
			"type": "object",
			"properties": {
				"filesystem": {
					"type": "string",
					"enum": ["ext2", "ext3", "ext4", "xfs"]
				},
			},
			"required": ["filesystem"]
		},
        "vagrant": {
            "type": "object",
            "properties": {
                "key.private": { "type": "string" },
                "key.public": { "type": "string" }
            }
        }
	},
	"required": ["volume"]
}
